{
  "name": "bod-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "clean": "react-native clean cache",
    "pod:install": "cd ios && rm -rf build && rm -rf Pods && rm -rf Podfile.lock && pod install --repo-update && cd ..",
    "android": "rm -rf android/build && rm -rf android/app/build && react-native run-android --variant=developmentDebug",
    "dev:win": "react-native run-android --variant=developmentDebug",
    "androidProd": "rm -rf android/build && rm -rf android/app/build && react-native run-android --variant=productionDebug",
    "androidRelease": "rm -rf android/build && rm -rf android/app/build && cd android && ./gradlew app:bundleProductionRelease && cd ..",
    "androidRelease-Dev": "cd android && ./gradlew app:bundleDevelopmentRelease && cd ..",
    "androidReleaseAPK": "rm -rf android/build && rm -rf android/app/build && cd android && ./gradlew app:assembleProductionRelease && cd ..",
    "androidReleaseAPK-Dev": "cd android && ./gradlew app:assembleDevelopmentRelease && cd .."
  },
